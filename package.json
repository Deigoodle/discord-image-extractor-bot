{
  "name": "discord-image-extractor-bot",
  "version": "1.0.0",
  "description": "Discord Bot that extract images from text-channels and save or upload them to a cloud service",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "register": "ts-node -r tsconfig-paths/register src/commands.ts",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:down": "docker-compose down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deigoodle/discord-image-extractor-bot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Deigoodle/discord-image-extractor-bot/issues"
  },
  "homepage": "https://github.com/Deigoodle/discord-image-extractor-bot#readme",
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "nodemon": "^3.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
